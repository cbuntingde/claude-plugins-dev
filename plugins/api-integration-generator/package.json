{
  "name": "api-integration-generator",
  "version": "1.0.0",
  "description": "Generate fully-typed, error-handled API client code from OpenAPI specs or documentation",
  "type": "module",
  "scripts": {
    "start": "node servers/api-docs-server/index.js",
    "check-updates": "node scripts/check-api-updates.js",
